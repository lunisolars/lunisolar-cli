{
  "name": "lunisolar-cli",
  "version": "0.3.0",
  "description": "This is a scaffold for quickly generating plugin project templates for lunisolar.js",
  "bin": {
    "lunisolar": "bin/index.js"
  },
  "main": "bin/index.js",
  "type": "module",
  "scripts": {
    "build": "rollup -c --configPlugin typescript",
    "dev": "rollup -wc --configPlugin typescript",
    "lint": "eslint --fix --ext .js,.ts src/ "
  },
  "keywords": [
    "lunisolar",
    "cli",
    "plugin"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lunisolar-js/lunisolar-cli"
  },
  "author": "Natron Kan (waterbeside)",
  "license": "MIT",
  "dependencies": {
    "commander": "^10.0.0",
    "kolorist": "^1.7.0",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.13.0",
    "@types/prompts": "^2.4.2",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.34.0",
    "rollup": "^3.15.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
